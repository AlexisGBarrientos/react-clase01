;\n    \n    return(\n        <Row>\n            {fetching \n            ?\n            <ClipLoader color={\"green\"} loading={fetching} css={override} size={150} /> \n            : characters.map((character) => (\n                <Character key={character.id} {...character} />\n            ))}\n        </Row>\n    );\n};\n\nexport default Characters;","/Users/alexisbarrientos/Desktop/react2021/react-clases/proyectos/personajes/src/Components/Characters/Character.js",["35"],"/Users/alexisbarrientos/Desktop/react2021/react-clases/proyectos/personajes/src/Components/Header/index.js",[],"/Users/alexisbarrientos/Desktop/react2021/react-clases/proyectos/personajes/src/Components/Header/Header.js",[],{"ruleId":"36","replacedBy":"37"},{"ruleId":"38","replacedBy":"39"},{"ruleId":"40","severity":1,"message":"41","line":21,"column":12,"nodeType":"42","messageId":"43","endLine":21,"endColumn":17},{"ruleId":"40","severity":1,"message":"44","line":1,"column":21,"nodeType":"42","messageId":"43","endLine":1,"endColumn":27},"no-native-reassign",["45"],"no-negated-in-lhs",["46"],"no-unused-vars","'error' is assigned a value but never used.","Identifier","unusedVar","'Button' is defined but never used.","no-global-assign","no-unsafe-negation"]